{
  "ConnectionStrings": {
    "DefaultConnection": "your_connection_string"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BotConfiguration": {
    "BotToken": "{BOT_TOKEN}",
    "HostAddress": "https://mydomain.com",
    "Route": "/bot",
    "SecretToken": "{YOUR-SECRET-TOKEN}"
  }
}
